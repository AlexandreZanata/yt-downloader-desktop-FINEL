{
  "name": "zanata-baixador",
  "version": "1.0.0",
  "description": "Aplicativo para baixar v√≠deos do YouTube",
  "author": "Seu Nome <seuemail@example.com>",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "dist": "electron-builder"
  },
  "devDependencies": {
    "electron": "^27.0.0",
    "electron-builder": "^23.6.0"
  },
  "build": {
    "appId": "com.seuapp.downloader",
    "files": [
      "**/*"
    ],
    "extraResources": [
      {
        "from": "resources/yt-dlp/",
        "to": "yt-dlp/",
        "filter": [
          "**/*"
        ]
      }
    ],
    "directories": {
      "buildResources": "resources"
    },
    "win": {
      "target": "nsis"
    },
    "icon": "resources/icon.png.ico"
  }
}
